{
  "name": "RostelekomTest",
  "version": "1.0.0",
  "description": "Test App For Rostelekom",
  "main": "server/server.js",
  "author": "Stolyarov Dmitriy",
  "license": "MIT",
  "scripts": {
    "test": "mocha ./tests/* --exit",
    "server": "nodemon server.js --config nodemon.json",
    "client": "yarn --cwd ../client start",
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "conc": "yarn add concurrently -D",
    "initInstall": "concurrently \"yarn install\" \"yarn --cwd ../client install\"",
    "restoreDb": "node ./migrate/migrateDb.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.1.9",
    "jsonwebtoken": "^8.5.1",
    "nconf": "^0.10.0",
    "ncp": "^2.0.0",
    "node-json-db": "^1.0.1",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "concurrently": "^4.1.1",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2"
  }
}
